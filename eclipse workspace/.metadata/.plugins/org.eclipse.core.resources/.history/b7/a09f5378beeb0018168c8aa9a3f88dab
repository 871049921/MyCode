
public class Lever1 extends Lever {
	
	public Lever1() {
		super();
	}
	
	public Lever1(int pos) {
		super(pos);
	}

	public int getPos() {
		return pos;
	}
	
	public void upPos() {
		if (this.pos < 4) {
			this.pos ++;
		}
	}
	
	public void downPos() {
		if (this.pos > 1) {
			this.pos --;
		}
	}
}
