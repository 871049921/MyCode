
public abstract class AgentState {
	private int state;
	
	public AgentState() {
		state = 1;
	}
	
	public int getState() {
		return this.state;
	}
	
	public void setState(int state) {
		this.state = state;
	}
	
	public abstract void state1();
	
	public abstract void state2();
	
	public abstract void state3();
	
	public abstract void state4();
}
